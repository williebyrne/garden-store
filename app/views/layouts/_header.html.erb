<header class="headSec">
      <h1>Silver Birch Online Garden Store</h1>
      
      <ul>
        <li><%= link_to "Home",  "/" %></li>
        <li><%= link_to "Products",  "/items" %></li>
        <li><%= link_to "View Cart",  "/cart" %></li>
        <li><%= link_to "About", about_path %></li>
        <li><%= link_to "Help", help_path %></li>
        
        
        
        
        <% if user_signed_in? %>
        
        <li><a href="/users/edit"> Hello <%= current_user.email %></a></li>
        <li><%= link_to "Log out", destroy_user_session_path, :method => :delete %></li>
        
        
        <div id="cartIcon">
        <li><a href="/cart"> (<%= current_user.email %>)</a></li>
        <li><a href="/cart"> (<%= @cart %>)</a></li>
        <li><%= link_to image_tag("cart.jpg", size: "30x30", :alt => "Shopping Cart"), '/cart' %></li>
        </div>
        
        
        <% else %>
        
        <li><%= link_to "Create account", new_user_registration_path %></li>
        <li><%= link_to "Log in", new_user_session_path %></li>
        
        <% end %>
        
        <li><a href="/admin">Admin</a></li>
        
        <% if user_signed_in? %>
  
        
        <li><a href="/users/edit">Update Profile</a></li>
        
        
        <% end %>
  
        
        
        
      </ul>
</header>